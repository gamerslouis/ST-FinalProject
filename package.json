{
  "name": "planeio",
  "version": "1.0.0",
  "author": "gamerslouis <gamerslouis@gmail.com>",
  "license": "MIT",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint --cache --ext .js,.jsx src/",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec ts-node src/server/index.ts",
    "start": "cross-env NODE_ENV=production nodemon --watch 'src/**/*.ts' --exec ts-node src/server/index.ts",
    "test": "yarn test:server && yarn test:client",
    "test:server": "jest --coverage --config=jest.config.server.js",
    "test:client": "jest --coverage --config=jest.config.client.js",
    "format": "prettier --write ./src"
  },
  "dependencies": {
    "express": "^4.18.1",
    "html-webpack-plugin": "^5.5.0",
    "jest-environment-jsdom": "^28.1.0",
    "lodash": "^4.17.21",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "throttle-debounce": "^5.0.0",
    "webpack": "^5.72.0",
    "webpack-dev-middleware": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.10",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.0.3",
    "mini-css-extract-plugin": "^2.6.0",
    "node-html-parser": "^5.3.3",
    "nodemon": "^2.0.16",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
